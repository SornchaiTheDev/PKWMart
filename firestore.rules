rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
    //   allow read, write: if
    //       request.time < timestamp.date(2021, 4, 28);
    // }
    allow read , write : if true;
    }
    
    match /stock/{document=**} {
    allow write , read : if true;
    }
  }
}